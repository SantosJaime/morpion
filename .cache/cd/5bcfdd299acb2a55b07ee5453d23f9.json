   this.board[cell] = this.currentPlayer ;\n        return true ;\n    }\n\n   /**\n    * Vérifie si le joueur courant a réaliser une condition de victoire.\n    *\n    * En cas de victoire, retourne la liste des cases gagnantes.\n    * Sinon retourne `false`.\n    *\n    * @returns {boolean|array}\n    Coup gagnant, sinon `false`.\n    */\n    checkWin() {\n        for (let pattern of WIN_PATTERNS) {\n            let match = true ;\n\n            for (let cell of pattern) {\n                if (this.board[cell] !== this.currentPlayer) {\n                    match = false ;\n                    break ;\n                }\n            }\n            if (match) { \n                return pattern ;\n            }\n        }\n        return false ;\n    }\n \n    /**\n    * Marque la partie comme terminée.\n    */\n    terminate() {\n        this.currentPlayer = null ;\n        \n        }\n        /**\n        * Vérifie si la partie a été marquée comme terminée.\n        */\n        isTerminated() {\n            return this.currentPlayer === null ;\n        }\n\n    /**\n    * Change le joueur courant.\n    *\n    * Marque la fin de partie si le nombre de coups maximum est atteint.\n    */\n    switchPlayer() {\n        if (this.movesCount === 9) {\n            this.terminate() ;\n            confirm(\"Égalité. On recommence?\");\n            pointEgalite++;\n            // affiche le score\n\t\t    pointEgaliteHTML.textContent = pointEgalite;\n            return ;\n        }\n\n        switch (this.currentPlayer) {\n            case PLAYER1 :\n                    tourJoueur= \"Tour de \"+this.nom1+\"(X)\";\n                    return tourJoueur;\n                this.currentPlayer = PLAYER2 ;\n                break ;\n\n            case PLAYER2 :\n                this.currentPlayer = PLAYER1 ;\n                break ;\n\n            case null :\n                break ;\n        }\n    }\n\n\n}\n\n//stocker score à égalité\nlet pointEgalite = 0;\n// récupérer l'affichage des scores\nlet pointJoueur1HTML = document.getElementById('pointJoueur1');\nlet pointJoueur2HTML = document.getElementById('pointJoueur2');\nlet pointEgaliteHTML = document.getElementById('pointEgalite');"},"lineCount":null}},"error":null,"hash":"8c27a8d5e9327bf8790553164f0380e3","cacheData":{"env":{}}}